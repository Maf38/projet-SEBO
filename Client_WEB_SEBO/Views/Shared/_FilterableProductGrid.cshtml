    <!-- top navbar -->




    <!-- Bottom bar with filter and cart info -->
    <div class="bar">
        <div class="filter">
            <span class="filter__label">Filter: </span>
            <button class="action filter__item filter__item--selected" data-filter="*">All</button>
            <button class="action filter__item" data-filter=".mafal"><i class="icon icon--jacket"></i><span class="action__text">CD</span></button>
            <button class="action filter__item" data-filter=".DVD"><i class="icon icon--shirt"></i><span class="action__text">DVD</span></button>
            <button class="action filter__item" data-filter=".Livre"><i class="icon icon--dress"></i><span class="action__text">Livre</span></button>
            
        </div>
        <button class="cart" data-filter=".dresses">
            <i class="cart__icon fa fa-shopping-cart"></i>
            <span class="text-hidden">Shopping cart</span>
            <span class="cart__count">0</span>
        </button>
    </div>
